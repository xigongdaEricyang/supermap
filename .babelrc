{
	"presets": [
		[
			"@babel/preset-env",
			{
				"modules": false,
				"useBuiltIns": "usage"
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-proposal-class-properties",
		[
			"import",
			{ "libraryName": "antd", "libraryDirectory": "es", "style": "css" }
    ]
	]
}
